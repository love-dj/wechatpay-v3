{
  "name": "love-dj/wechatpay-v3",
  "description": "用于框架的微信支付 API v3 组件",
  "authors": [
    {
      "name": "心情love",
      "email": "myhsj@qq.com"
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-openssl": "*",
    "ext-redis": "*",
    "guzzlehttp/guzzle": "^7.5",
    "pimple/pimple": "^3.5",
    "illuminate/support": "^8.83",
    "monolog/monolog": "^2.8",
    "phpoption/phpoption": "^1.8",
    "vlucas/phpdotenv": "^5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload": {
    "psr-4": {
      "Love\\WeChatPayV3\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Love\\WeChatPayV3\\Test\\": "tests"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  }
}
